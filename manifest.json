{
    "manifest_version": 3,
    "name": "GitHub to CodeSandbox Redirector",
    "version": "1.0",
    "description": "Paste a GitHub repo URL to open it in CodeSandbox.",
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icon16.png",
        "32": "icon32.png",
        "48": "icon48.png",
        "128": "icon128.png"
      }
    },
    "permissions": ["tabs"]
  }
  